<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY bfo "https://ontology.brasil.gov.br/bfo#">
    <!ENTITY fibo-fnd "https://spec.edmcouncil.org/fibo/ontology/FND/">
    <!ENTITY fibo-be "https://spec.edmcouncil.org/fibo/ontology/BE/">
    <!ENTITY fibo-fbc "https://spec.edmcouncil.org/fibo/ontology/FBC/">
    <!ENTITY fibo-sec "https://spec.edmcouncil.org/fibo/ontology/SEC/">
    <!ENTITY ofb "https://openfinancebrasil.org.br/schema/">
    <!ENTITY bcb "https://dadosabertos.bcb.gov.br/schema/">
    <!ENTITY cvm "https://dados.cvm.gov.br/schema/">
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:bfo="&bfo;"
    xmlns:fibo-fnd="&fibo-fnd;"
    xmlns:fibo-be="&fibo-be;"
    xmlns:fibo-fbc="&fibo-fbc;"
    xmlns:fibo-sec="&fibo-sec;"
    xmlns:ofb="&ofb;"
    xmlns:bcb="&bcb;"
    xmlns:cvm="&cvm;">

    <!-- ==========================================
         ONTOLOGY METADATA
         ========================================== -->

    <owl:Ontology rdf:about="&bfo;">
        <rdfs:label xml:lang="pt-BR">Brazilian Financial Ontology (BFO)</rdfs:label>
        <rdfs:label xml:lang="en">Brazilian Financial Ontology (BFO)</rdfs:label>
        <dcterms:title xml:lang="pt-BR">Ontologia Financeira Brasileira</dcterms:title>
        <dcterms:description xml:lang="pt-BR">
            Ontologia formal do sistema financeiro brasileiro, alinhada com FIBO
            (Financial Industry Business Ontology) e integrada com os padrões do
            Open Finance Brasil e esquemas do Banco Central do Brasil.
        </dcterms:description>
        <dcterms:creator>Brazilian Financial Ontology Consortium</dcterms:creator>
        <dcterms:created rdf:datatype="&xsd;date">2026-01-15</dcterms:created>
        <owl:versionInfo>1.0.0</owl:versionInfo>
        <dcterms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/" />

        <!-- FIBO Imports -->
        <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/" />
        <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/" />
        <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/" />
        <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/" />
    </owl:Ontology>

    <!-- ==========================================
         PART 1: BRAZILIAN REGULATORY BODIES
         Alinhado com FIBO-FBC (Financial Business and Commerce)
         ========================================== -->

    <!-- Classe Base: Órgão Regulador Brasileiro -->
    <owl:Class rdf:about="&bfo;OrgaoReguladorBrasileiro">
        <rdfs:subClassOf rdf:resource="&fibo-fbc;RegulatoryAgency" />
        <rdfs:label xml:lang="pt-BR">Órgão Regulador Brasileiro</rdfs:label>
        <rdfs:label xml:lang="en">Brazilian Regulatory Body</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Entidade governamental brasileira responsável pela regulação e
            supervisão do sistema financeiro nacional.
        </skos:definition>
    </owl:Class>

    <!-- Banco Central do Brasil -->
    <owl:NamedIndividual rdf:about="&bfo;BancoCentralDoBrasil">
        <rdf:type rdf:resource="&bfo;OrgaoReguladorBrasileiro" />
        <rdfs:label xml:lang="pt-BR">Banco Central do Brasil</rdfs:label>
        <skos:altLabel>BCB</skos:altLabel>
        <skos:altLabel>BACEN</skos:altLabel>
        <bfo:codigoISPB>00000000</bfo:codigoISPB>
        <bfo:cnpj>00038166000105</bfo:cnpj>
        <rdfs:seeAlso rdf:resource="https://www.bcb.gov.br" />
        <skos:definition xml:lang="pt-BR">
            Autarquia federal vinculada ao Ministério da Fazenda, responsável
            pela política monetária, cambial e regulação do sistema financeiro.
        </skos:definition>
    </owl:NamedIndividual>

    <!-- Comissão de Valores Mobiliários -->
    <owl:NamedIndividual rdf:about="&bfo;ComissaoDeValoresMobiliarios">
        <rdf:type rdf:resource="&bfo;OrgaoReguladorBrasileiro" />
        <rdfs:label xml:lang="pt-BR">Comissão de Valores Mobiliários</rdfs:label>
        <skos:altLabel>CVM</skos:altLabel>
        <bfo:cnpj>29507878000194</bfo:cnpj>
        <rdfs:seeAlso rdf:resource="https://www.cvm.gov.br" />
        <skos:definition xml:lang="pt-BR">
            Autarquia vinculada ao Ministério da Fazenda responsável pela
            regulamentação e fiscalização do mercado de valores mobiliários.
        </skos:definition>
    </owl:NamedIndividual>

    <!-- SUSEP -->
    <owl:NamedIndividual rdf:about="&bfo;SUSEP">
        <rdf:type rdf:resource="&bfo;OrgaoReguladorBrasileiro" />
        <rdfs:label xml:lang="pt-BR">Superintendência de Seguros Privados</rdfs:label>
        <skos:altLabel>SUSEP</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Autarquia responsável pelo controle e fiscalização dos mercados
            de seguro, previdência privada aberta e capitalização.
        </skos:definition>
    </owl:NamedIndividual>

    <!-- PREVIC -->
    <owl:NamedIndividual rdf:about="&bfo;PREVIC">
        <rdf:type rdf:resource="&bfo;OrgaoReguladorBrasileiro" />
        <rdfs:label xml:lang="pt-BR">Superintendência Nacional de Previdência Complementar</rdfs:label>
        <skos:altLabel>PREVIC</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Autarquia responsável pela fiscalização das entidades fechadas
            de previdência complementar (fundos de pensão).
        </skos:definition>
    </owl:NamedIndividual>

    <!-- ==========================================
         PART 2: FINANCIAL INSTITUTIONS
         Alinhado com FIBO-BE (Business Entities) e FIBO-FBC
         ========================================== -->

    <!-- Classe Base: Instituição Financeira Brasileira -->
    <owl:Class rdf:about="&bfo;InstituicaoFinanceiraBrasileira">
        <rdfs:subClassOf rdf:resource="&fibo-fbc;FinancialServiceProvider" />
        <rdfs:label xml:lang="pt-BR">Instituição Financeira Brasileira</rdfs:label>
        <rdfs:label xml:lang="en">Brazilian Financial Institution</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Pessoa jurídica autorizada pelo Banco Central do Brasil a operar
            no Sistema Financeiro Nacional.
        </skos:definition>
    </owl:Class>

    <!-- Banco Comercial -->
    <owl:Class rdf:about="&bfo;BancoComercial">
        <rdfs:subClassOf rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:subClassOf rdf:resource="&fibo-fbc;DepositoryInstitution" />
        <rdfs:label xml:lang="pt-BR">Banco Comercial</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bfo;InstituicaoFinanceiraBrasileira" />
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bfo;tipoCarteiraBCB" />
                        <owl:hasValue>comercial</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <skos:definition xml:lang="pt-BR">
            Instituição financeira autorizada a captar depósitos à vista e
            realizar operações de crédito de curto e médio prazo.
        </skos:definition>
        <!-- Mapeamento Open Finance Brasil -->
        <owl:equivalentClass rdf:resource="&ofb;BancoComercial" />
    </owl:Class>

    <!-- Banco de Investimento -->
    <owl:Class rdf:about="&bfo;BancoDeInvestimento">
        <rdfs:subClassOf rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:subClassOf rdf:resource="&fibo-fbc;InvestmentBank" />
        <rdfs:label xml:lang="pt-BR">Banco de Investimento</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituição especializada em operações de participação ou
            financiamento de médio e longo prazo para capital de giro
            ou ativo permanente de empresas.
        </skos:definition>
    </owl:Class>

    <!-- Banco Múltiplo -->
    <owl:Class rdf:about="&bfo;BancoMultiplo">
        <rdfs:subClassOf rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:label xml:lang="pt-BR">Banco Múltiplo</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituição financeira que opera com pelo menos duas carteiras,
            sendo uma obrigatoriamente comercial ou de investimento.
        </skos:definition>
        <rdfs:comment xml:lang="pt-BR">
            Deve possuir ao menos duas das seguintes carteiras: comercial,
            investimento, crédito imobiliário, crédito/financiamento/investimento,
            arrendamento mercantil.
        </rdfs:comment>
    </owl:Class>

    <!-- Cooperativa de Crédito -->
    <owl:Class rdf:about="&bfo;CooperativaDeCredito">
        <rdfs:subClassOf rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:subClassOf rdf:resource="&fibo-fbc;CreditUnion" />
        <rdfs:label xml:lang="pt-BR">Cooperativa de Crédito</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituição financeira constituída sob a forma de sociedade
            cooperativa, destinada a prestar serviços financeiros aos
            associados.
        </skos:definition>
        <owl:equivalentClass rdf:resource="&ofb;CooperativaCredito" />
    </owl:Class>

    <!-- Instituição de Pagamento (Fintech) -->
    <owl:Class rdf:about="&bfo;InstituicaoDePagamento">
        <rdfs:subClassOf rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:label xml:lang="pt-BR">Instituição de Pagamento</rdfs:label>
        <skos:altLabel>IP</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Pessoa jurídica que executa serviços de pagamento, incluindo
            emissores de moeda eletrônica, iniciadores de pagamento e
            credenciadores.
        </skos:definition>
        <rdfs:comment xml:lang="pt-BR">
            Regulamentada pela Lei 12.865/2013 e Circular BCB 3.885/2018.
        </rdfs:comment>
    </owl:Class>

    <!-- Corretora de Títulos e Valores Mobiliários -->
    <owl:Class rdf:about="&bfo;CorretoraDeTVM">
        <rdfs:subClassOf rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:subClassOf rdf:resource="&fibo-fbc;BrokerDealer" />
        <rdfs:label xml:lang="pt-BR">Corretora de Títulos e Valores Mobiliários</rdfs:label>
        <skos:altLabel>CTVM</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Sociedade autorizada a operar em bolsas de valores e mercados
            de balcão organizados, intermediando a compra e venda de
            valores mobiliários.
        </skos:definition>
    </owl:Class>

    <!-- Distribuidora de Títulos e Valores Mobiliários -->
    <owl:Class rdf:about="&bfo;DistribuidoraDeTVM">
        <rdfs:subClassOf rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:label xml:lang="pt-BR">Distribuidora de Títulos e Valores Mobiliários</rdfs:label>
        <skos:altLabel>DTVM</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Sociedade autorizada a intermediar a distribuição de títulos
            e valores mobiliários no mercado.
        </skos:definition>
    </owl:Class>

    <!-- Segmentação Prudencial BCB (S1-S5) -->
    <owl:Class rdf:about="&bfo;SegmentacaoPrudencial">
        <rdfs:label xml:lang="pt-BR">Segmentação Prudencial</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Classificação das instituições financeiras em segmentos (S1 a S5)
            conforme porte, atividade internacional e perfil de risco,
            definida pela Resolução BCB 4.553/2017.
        </skos:definition>
    </owl:Class>

    <owl:NamedIndividual rdf:about="&bfo;SegmentoS1">
        <rdf:type rdf:resource="&bfo;SegmentacaoPrudencial" />
        <rdfs:label>S1</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituições de porte igual ou superior a 10% do PIB ou com
            atividade internacional relevante.
        </skos:definition>
        <bfo:exposicaoMinima rdf:datatype="&xsd;decimal">0.10</bfo:exposicaoMinima>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;SegmentoS2">
        <rdf:type rdf:resource="&bfo;SegmentacaoPrudencial" />
        <rdfs:label>S2</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituições de porte entre 1% e 10% do PIB.
        </skos:definition>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;SegmentoS3">
        <rdf:type rdf:resource="&bfo;SegmentacaoPrudencial" />
        <rdfs:label>S3</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituições de porte entre 0,1% e 1% do PIB.
        </skos:definition>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;SegmentoS4">
        <rdf:type rdf:resource="&bfo;SegmentacaoPrudencial" />
        <rdfs:label>S4</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituições de porte inferior a 0,1% do PIB.
        </skos:definition>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;SegmentoS5">
        <rdf:type rdf:resource="&bfo;SegmentacaoPrudencial" />
        <rdfs:label>S5</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Instituições de porte inferior a 0,1% do PIB e perfil de
            risco simplificado.
        </skos:definition>
    </owl:NamedIndividual>

    <!-- ==========================================
         PART 3: LEGAL ENTITIES (PARTIES)
         Alinhado com FIBO-BE (Business Entities)
         ========================================== -->

    <!-- Pessoa (Party) -->
    <owl:Class rdf:about="&bfo;Pessoa">
        <rdfs:subClassOf rdf:resource="&fibo-fnd;Party" />
        <rdfs:label xml:lang="pt-BR">Pessoa</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Entidade capaz de ser titular de direitos e obrigações no
            sistema financeiro brasileiro.
        </skos:definition>
    </owl:Class>

    <!-- Pessoa Física -->
    <owl:Class rdf:about="&bfo;PessoaFisica">
        <rdfs:subClassOf rdf:resource="&bfo;Pessoa" />
        <rdfs:subClassOf rdf:resource="&fibo-be;NaturalPerson" />
        <rdfs:label xml:lang="pt-BR">Pessoa Física</rdfs:label>
        <rdfs:label xml:lang="en">Natural Person</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Indivíduo natural identificado pelo CPF no sistema brasileiro.
        </skos:definition>
        <!-- Mapeamento Open Finance Brasil -->
        <owl:equivalentClass rdf:resource="&ofb;NaturalPerson" />
    </owl:Class>

    <!-- Pessoa Jurídica -->
    <owl:Class rdf:about="&bfo;PessoaJuridica">
        <rdfs:subClassOf rdf:resource="&bfo;Pessoa" />
        <rdfs:subClassOf rdf:resource="&fibo-be;LegalEntity" />
        <rdfs:label xml:lang="pt-BR">Pessoa Jurídica</rdfs:label>
        <rdfs:label xml:lang="en">Legal Entity</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Entidade com personalidade jurídica própria, identificada
            pelo CNPJ no sistema brasileiro.
        </skos:definition>
        <owl:equivalentClass rdf:resource="&ofb;BusinessEntity" />
    </owl:Class>

    <!-- Classificação de Investidor CVM -->
    <owl:Class rdf:about="&bfo;ClassificacaoInvestidor">
        <rdfs:label xml:lang="pt-BR">Classificação de Investidor</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Categorização de investidores conforme a Resolução CVM 30/2021.
        </skos:definition>
    </owl:Class>

    <owl:NamedIndividual rdf:about="&bfo;InvestidorProfissional">
        <rdf:type rdf:resource="&bfo;ClassificacaoInvestidor" />
        <rdfs:label xml:lang="pt-BR">Investidor Profissional</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Investidor com aplicações financeiras superiores a R$ 10 milhões
            ou certificação técnica aprovada pela CVM.
        </skos:definition>
        <bfo:valorMinimoAplicacoes rdf:datatype="&xsd;decimal">10000000</bfo:valorMinimoAplicacoes>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;InvestidorQualificado">
        <rdf:type rdf:resource="&bfo;ClassificacaoInvestidor" />
        <rdfs:label xml:lang="pt-BR">Investidor Qualificado</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Investidor com aplicações financeiras superiores a R$ 1 milhão
            ou certificação técnica aprovada pela CVM.
        </skos:definition>
        <bfo:valorMinimoAplicacoes rdf:datatype="&xsd;decimal">1000000</bfo:valorMinimoAplicacoes>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;InvestidorVarejo">
        <rdf:type rdf:resource="&bfo;ClassificacaoInvestidor" />
        <rdfs:label xml:lang="pt-BR">Investidor de Varejo</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Investidor que não se enquadra nas categorias de profissional
            ou qualificado.
        </skos:definition>
    </owl:NamedIndividual>

    <!-- ==========================================
         PART 4: FINANCIAL INSTRUMENTS
         Alinhado com FIBO-SEC (Securities) e FIBO-FND
         ========================================== -->

    <!-- Classe Base: Instrumento Financeiro -->
    <owl:Class rdf:about="&bfo;InstrumentoFinanceiro">
        <rdfs:subClassOf rdf:resource="&fibo-fnd;FinancialInstrument" />
        <rdfs:label xml:lang="pt-BR">Instrumento Financeiro</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Ativo financeiro negociável no mercado brasileiro.
        </skos:definition>
    </owl:Class>

    <!-- ========== RENDA FIXA ========== -->

    <owl:Class rdf:about="&bfo;InstrumentoRendaFixa">
        <rdfs:subClassOf rdf:resource="&bfo;InstrumentoFinanceiro" />
        <rdfs:subClassOf rdf:resource="&fibo-sec;DebtInstrument" />
        <rdfs:label xml:lang="pt-BR">Instrumento de Renda Fixa</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Título de dívida com fluxo de pagamentos predefinido.
        </skos:definition>
    </owl:Class>

    <!-- Títulos Públicos Federais (Tesouro Direto) -->
    <owl:Class rdf:about="&bfo;TituloPublicoFederal">
        <rdfs:subClassOf rdf:resource="&bfo;InstrumentoRendaFixa" />
        <rdfs:subClassOf rdf:resource="&fibo-sec;GovernmentDebtSecurity" />
        <rdfs:label xml:lang="pt-BR">Título Público Federal</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Título de dívida emitido pelo Tesouro Nacional.
        </skos:definition>
        <bfo:emissor rdf:resource="&bfo;TesouroNacional" />
    </owl:Class>

    <owl:Class rdf:about="&bfo;TesouroSelic">
        <rdfs:subClassOf rdf:resource="&bfo;TituloPublicoFederal" />
        <rdfs:label xml:lang="pt-BR">Tesouro Selic</rdfs:label>
        <skos:altLabel>LFT</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Título pós-fixado indexado à taxa Selic.
        </skos:definition>
        <bfo:indexador rdf:resource="&bfo;TaxaSelic" />
        <bfo:tipoRemuneracao>pos-fixado</bfo:tipoRemuneracao>
    </owl:Class>

    <owl:Class rdf:about="&bfo;TesouroIPCA">
        <rdfs:subClassOf rdf:resource="&bfo;TituloPublicoFederal" />
        <rdfs:label xml:lang="pt-BR">Tesouro IPCA+</rdfs:label>
        <skos:altLabel>NTN-B</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Título híbrido com rentabilidade atrelada ao IPCA mais
            uma taxa prefixada.
        </skos:definition>
        <bfo:indexador rdf:resource="&bfo;IPCA" />
        <bfo:tipoRemuneracao>hibrido</bfo:tipoRemuneracao>
    </owl:Class>

    <owl:Class rdf:about="&bfo;TesouroPrefixado">
        <rdfs:subClassOf rdf:resource="&bfo;TituloPublicoFederal" />
        <rdfs:label xml:lang="pt-BR">Tesouro Prefixado</rdfs:label>
        <skos:altLabel>LTN</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Título com rentabilidade definida no momento da compra.
        </skos:definition>
        <bfo:tipoRemuneracao>prefixado</bfo:tipoRemuneracao>
    </owl:Class>

    <!-- Títulos Privados -->
    <owl:Class rdf:about="&bfo;TituloPrivadoRendaFixa">
        <rdfs:subClassOf rdf:resource="&bfo;InstrumentoRendaFixa" />
        <rdfs:label xml:lang="pt-BR">Título Privado de Renda Fixa</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="&bfo;CDB">
        <rdfs:subClassOf rdf:resource="&bfo;TituloPrivadoRendaFixa" />
        <rdfs:subClassOf rdf:resource="&fibo-sec;CertificateOfDeposit" />
        <rdfs:label xml:lang="pt-BR">Certificado de Depósito Bancário</rdfs:label>
        <skos:altLabel>CDB</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Título de renda fixa emitido por bancos para captação de recursos.
        </skos:definition>
        <bfo:coberturaFGC rdf:datatype="&xsd;boolean">true</bfo:coberturaFGC>
        <bfo:limiteFGC rdf:datatype="&xsd;decimal">250000</bfo:limiteFGC>
    </owl:Class>

    <owl:Class rdf:about="&bfo;LCI">
        <rdfs:subClassOf rdf:resource="&bfo;TituloPrivadoRendaFixa" />
        <rdfs:label xml:lang="pt-BR">Letra de Crédito Imobiliário</rdfs:label>
        <skos:altLabel>LCI</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Título lastreado em créditos imobiliários, isento de IR para
            pessoa física.
        </skos:definition>
        <bfo:isentoIRPF rdf:datatype="&xsd;boolean">true</bfo:isentoIRPF>
        <bfo:coberturaFGC rdf:datatype="&xsd;boolean">true</bfo:coberturaFGC>
    </owl:Class>

    <owl:Class rdf:about="&bfo;LCA">
        <rdfs:subClassOf rdf:resource="&bfo;TituloPrivadoRendaFixa" />
        <rdfs:label xml:lang="pt-BR">Letra de Crédito do Agronegócio</rdfs:label>
        <skos:altLabel>LCA</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Título lastreado em créditos do agronegócio, isento de IR
            para pessoa física.
