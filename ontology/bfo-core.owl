    <owl:ObjectProperty rdf:about="&bfo;realizadaPor">
        <rdfs:domain rdf:resource="&bfo;OperacaoFinanceira" />
        <rdfs:range rdf:resource="&bfo;Pessoa" />
        <rdfs:label xml:lang="pt-BR">realizada por</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bfo;envolveinstrumento">
        <rdfs:domain rdf:resource="&bfo;OperacaoFinanceira" />
        <rdfs:range rdf:resource="&bfo;InstrumentoFinanceiro" />
        <rdfs:label xml:lang="pt-BR">envolve instrumento</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bfo;emitidoPor">
        <rdfs:domain rdf:resource="&bfo;InstrumentoFinanceiro" />
        <rdfs:range rdf:resource="&bfo;PessoaJuridica" />
        <rdfs:label xml:lang="pt-BR">emitido por</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&fibo-sec;isIssuedBy" />
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bfo;custodiado Por">
        <rdfs:domain rdf:resource="&bfo;InstrumentoFinanceiro" />
        <rdfs:range rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:label xml:lang="pt-BR">custodiado por</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bfo;indexadoPor">
        <rdfs:domain rdf:resource="&bfo;InstrumentoRendaFixa" />
        <rdfs:range rdf:resource="&bfo;IndicadorEconomico" />
        <rdfs:label xml:lang="pt-BR">indexado por</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bfo;segmentoPrudencial">
        <rdfs:domain rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:range rdf:resource="&bfo;SegmentacaoPrudencial" />
        <rdfs:label xml:lang="pt-BR">segmento prudencial</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bfo;classificacaoInvestidor">
        <rdfs:domain rdf:resource="&bfo;Pessoa" />
        <rdfs:range rdf:resource="&bfo;ClassificacaoInvestidor" />
        <rdfs:label xml:lang="pt-BR">classificação de investidor</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bfo;classificacaoRisco">
        <rdfs:domain rdf:resource="&bfo;OperacaoCredito" />
        <rdfs:range rdf:resource="&bfo;ClassificacaoRiscoCredito" />
        <rdfs:label xml:lang="pt-BR">classificação de risco</rdfs:label>
    </owl:ObjectProperty>

    <!-- ========== Data Properties ========== -->

    <!-- Identificadores Brasileiros -->
    <owl:DatatypeProperty rdf:about="&bfo;cpf">
        <rdfs:domain rdf:resource="&bfo;PessoaFisica" />
        <rdfs:range rdf:resource="&xsd;string" />
        <rdfs:label xml:lang="pt-BR">CPF</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Cadastro de Pessoas Físicas - identificador único de pessoa
            física no Brasil (11 dígitos).
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be;hasLegalEntityIdentifier" />
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;cnpj">
        <rdfs:domain rdf:resource="&bfo;PessoaJuridica" />
        <rdfs:range rdf:resource="&xsd;string" />
        <rdfs:label xml:lang="pt-BR">CNPJ</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Cadastro Nacional da Pessoa Jurídica - identificador único
            de pessoa jurídica no Brasil (14 dígitos).
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be;hasLegalEntityIdentifier" />
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;codigoISPB">
        <rdfs:domain rdf:resource="&bfo;InstituicaoFinanceiraBrasileira" />
        <rdfs:range rdf:resource="&xsd;string" />
        <rdfs:label xml:lang="pt-BR">Código ISPB</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Identificador do Sistema de Pagamentos Brasileiro (8 dígitos).
        </skos:definition>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;codigoISIN">
        <rdfs:domain rdf:resource="&bfo;InstrumentoFinanceiro" />
        <rdfs:range rdf:resource="&xsd;string" />
        <rdfs:label xml:lang="pt-BR">Código ISIN</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            International Securities Identification Number (12 caracteres).
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-sec;hasISIN" />
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;ticker">
        <rdfs:domain rdf:resource="&bfo;InstrumentoFinanceiro" />
        <rdfs:range rdf:resource="&xsd;string" />
        <rdfs:label xml:lang="pt-BR">Ticker</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Código de negociação do ativo em bolsa.
        </skos:definition>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;chavePIX">
        <rdfs:domain rdf:resource="&bfo;Conta" />
        <rdfs:range rdf:resource="&xsd;string" />
        <rdfs:label xml:lang="pt-BR">Chave PIX</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Identificador único para recebimento de PIX (CPF/CNPJ,
            e-mail, telefone ou chave aleatória).
        </skos:definition>
    </owl:DatatypeProperty>

    <!-- Valores Monetários -->
    <owl:DatatypeProperty rdf:about="&bfo;valorBRL">
        <rdfs:range rdf:resource="&xsd;decimal" />
        <rdfs:label xml:lang="pt-BR">Valor em BRL</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Valor monetário expresso em Reais brasileiros.
        </skos:definition>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;taxaJuros">
        <rdfs:range rdf:resource="&xsd;decimal" />
        <rdfs:label xml:lang="pt-BR">Taxa de Juros</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Taxa de juros expressa em percentual ao ano.
        </skos:definition>
    </owl:DatatypeProperty>

    <!-- Datas -->
    <owl:DatatypeProperty rdf:about="&bfo;dataOperacao">
        <rdfs:domain rdf:resource="&bfo;OperacaoFinanceira" />
        <rdfs:range rdf:resource="&xsd;dateTime" />
        <rdfs:label xml:lang="pt-BR">Data da Operação</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;dataLiquidacao">
        <rdfs:domain rdf:resource="&bfo;OperacaoFinanceira" />
        <rdfs:range rdf:resource="&xsd;date" />
        <rdfs:label xml:lang="pt-BR">Data de Liquidação</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;dataVencimento">
        <rdfs:domain rdf:resource="&bfo;InstrumentoRendaFixa" />
        <rdfs:range rdf:resource="&xsd;date" />
        <rdfs:label xml:lang="pt-BR">Data de Vencimento</rdfs:label>
    </owl:DatatypeProperty>

    <!-- Atributos Booleanos -->
    <owl:DatatypeProperty rdf:about="&bfo;coberturaFGC">
        <rdfs:range rdf:resource="&xsd;boolean" />
        <rdfs:label xml:lang="pt-BR">Cobertura FGC</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Indica se o instrumento possui cobertura do Fundo
            Garantidor de Créditos.
        </skos:definition>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&bfo;isentoIRPF">
        <rdfs:range rdf:resource="&xsd;boolean" />
        <rdfs:label xml:lang="pt-BR">Isento de IRPF</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Indica se o instrumento é isento de Imposto de Renda
            para pessoa física.
        </skos:definition>
    </owl:DatatypeProperty>

    <!-- ==========================================
         PART 9: TAXATION
         ========================================== -->

    <owl:Class rdf:about="&bfo;Tributo">
        <rdfs:label xml:lang="pt-BR">Tributo</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Obrigação pecuniária compulsória devida ao Estado.
        </skos:definition>
    </owl:Class>

    <owl:NamedIndividual rdf:about="&bfo;IR_RendaFixa">
        <rdf:type rdf:resource="&bfo;Tributo" />
        <rdfs:label xml:lang="pt-BR">IR sobre Renda Fixa</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Imposto de Renda retido na fonte sobre rendimentos de
            aplicações em renda fixa, com alíquotas regressivas.
        </skos:definition>
        <bfo:tabelaRegressiva rdf:datatype="&xsd;boolean">true</bfo:tabelaRegressiva>
        <rdfs:comment xml:lang="pt-BR">
            Alíquotas: 22,5% (até 180 dias), 20% (181-360),
            17,5% (361-720), 15% (acima de 720 dias).
        </rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;IR_RendaVariavel">
        <rdf:type rdf:resource="&bfo;Tributo" />
        <rdfs:label xml:lang="pt-BR">IR sobre Renda Variável</rdfs:label>
        <skos:definition xml:lang="pt-BR">
            Imposto de Renda sobre ganhos de capital em operações
            com ações e derivativos.
        </skos:definition>
        <rdfs:comment xml:lang="pt-BR">
            Alíquota de 15% para operações normais e 20% para day-trade.
            Isenção mensal para vendas de ações até R$ 20.000.
        </rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&bfo;IOF">
        <rdf:type rdf:resource="&bfo;Tributo" />
        <rdfs:label xml:lang="pt-BR">IOF</rdfs:label>
        <skos:altLabel>Imposto sobre Operações Financeiras</skos:altLabel>
        <skos:definition xml:lang="pt-BR">
            Imposto federal sobre operações de crédito, câmbio,
            seguros e títulos mobiliários.
        </skos:definition>
    </owl:NamedIndividual>

    <owl:ObjectProperty rdf:about="&bfo;incideTributo">
        <rdfs:domain rdf:resource="&bfo;OperacaoFinanceira" />
        <rdfs:range rdf:resource="&bfo;Tributo" />
        <rdfs:label xml:lang="pt-BR">incide tributo</rdfs:label>
    </owl:ObjectProperty>

</rdf:RDF>